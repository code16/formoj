{
  "name": "code16/formoj",
  "description": "Customizable form renderer",
  "authors": [{
    "name": "Philippe Lonchampt",
    "email": "philippe@code16.fr"
  }, {
    "name": "Antoine Guingand",
    "email": "antoine@code16.fr"
  }],
  "require": {
    "php": "^8.2|^8.3|^8.4",
    "laravel/framework": "^^10.0|^11.0",
    "nesbot/carbon": "^2.0",
    "maatwebsite/excel": "^3.1",
    "ext-zip": "*"
  },
  "require-dev": {
    "fakerphp/faker": "^1.19.0",
    "mockery/mockery": "^1.3.0",
    "phpunit/phpunit": "^10.0",
    "doctrine/dbal": "^3.0",
    "orchestra/testbench": "6.*|7.*|8.*|9.*",
    "code16/sharp": "^9.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Code16\\Formoj\\": "src/",
      "Code16\\Formoj\\Database\\Factories\\": "database/factories/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
      "Code16\\Formoj\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Code16\\Formoj\\FormojServiceProvider"
      ]
    }
  }
}
